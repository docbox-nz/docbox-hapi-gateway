{
  "name": "@docbox-nz/hapi-gateway",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "watch": "rollup -c --watch"
  },
  "main": "./dist/index.browser.cjs",
  "types": "./dist/index.d.ts",
